    </select>

    <select id="selectUsers" resultMap="BaseResultMap">
        select u.id as id ,u.userId as userId, u.username as username,u.nickname as nickname,u.canteen_id as canteen_id,
        u.telephone as telephone,CASE u.status WHEN 0 THEN '在职' WHEN 1 then '休假' when 2 then '离职'
        end AS state,u.status as status, u.remark as remark,u.connectperson as connectperson,r.role_name as
        role_name,r.id as rid from
        (select *from t_user where is_delete=0
        <if test="user.userType">
            and tenant_id = #{user.tenantId}
        </if>
        <if test="keyword!=''">
            and username like #{keyword} OR telephone LIKE #{keyword} OR nickname LIKE #{keyword} OR userId LIKE
            #{keyword}
        </if>
        )
        as u INNER JOIN t_role as r on r.id=u.role_id limit #{offSet},#{pageSize}
